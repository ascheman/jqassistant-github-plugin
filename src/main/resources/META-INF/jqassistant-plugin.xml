<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v2.4"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://schema.jqassistant.org/plugin/v2.4 https://jqassistant.github.io/jqassistant/current/schema/jqassistant-plugin-v2.4.xsd"
                    name="jQAssistant GitHub Plugin" id="jqa.plugin.github"
                    version="${project.version}">
    <description>${project.description}</description>
    <model>
        <class>org.jqassistant.plugin.github.model.GitHub</class>
        <class>org.jqassistant.plugin.github.model.GitHubBranch</class>
        <class>org.jqassistant.plugin.github.model.GitHubCommit</class>
        <class>org.jqassistant.plugin.github.model.GitHubIssue</class>
        <class>org.jqassistant.plugin.github.model.GitHubLabel</class>
        <class>org.jqassistant.plugin.github.model.GitHubMilestone</class>
        <class>org.jqassistant.plugin.github.model.GitHubRelease</class>
        <class>org.jqassistant.plugin.github.model.GitHubTag</class>
        <class>org.jqassistant.plugin.github.model.GitHubRepository</class>
        <class>org.jqassistant.plugin.github.model.GitHubUser</class>
        <class>org.jqassistant.plugin.github.model.GitHubPullRequest</class>
    </model>
    <scope>
        <class>org.jqassistant.plugin.github.scanner.GitHubScope</class>
    </scope>
    <scanner>
        <class>org.jqassistant.plugin.github.scanner.GitHubIssueScannerPlugin</class>
        <class>org.jqassistant.plugin.github.scanner.GHRepositoryScannerPlugin</class>
    </scanner>
</jqassistant-plugin>
